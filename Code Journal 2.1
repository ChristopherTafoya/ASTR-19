   "source": [
    "# Use the function to compute y = exp(x)\n",
    "y = exp_func(x)\n",
    "\n",
    "# Plot x vs y\n",
    "plt.figure(figsize=(6,4))\n",
    "plt.plot(x, y, label='y = exp(x)')\n",
    "plt.xlabel(\"Time [milliseconds]\")\n",
    "plt.ylabel(\"Awesomeness\")\n",
    "plt.title(\"Exponential Growth of Awesomeness Over Time\")\n",
    "plt.legend()\n",
