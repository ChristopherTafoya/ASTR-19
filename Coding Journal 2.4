    "# Create a histogram with 100 bins\n",
    "plt.figure(figsize=(6,4))\n",
    "plt.hist(data, bins=100, color='lightgreen', edgecolor='black')\n",
    "\n",
    "# Label the axes and add a title\n",
    "plt.xlabel(\"Value\")\n",
    "plt.ylabel(\"Frequency\")\n",
