import numpy as np

def main():
    # Number of entries
    num_entries = 1000

    # Generate 1000 values of x between 0 and 2
    x_values = np.linspace(0, 2, num_entries)
    
    # Compute sin(x) for each x
    sin_values = np.sin(x_values)
    
    # Print table header
    print(f"{'x':>10} {'sin(x)':>10}")
    print("-" * 22)
    
    # Print the table of x vs sin(x)
    for x, sin_x in zip(x_values, sin_values):
        print(f"{x:10.5f} {sin_x:10.5f}")

if __name__ == "__main__":
    main()
